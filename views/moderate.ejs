<!doctype html>
<html prefix="og: http://ogp.me/ns#">
    <head>
    <% include ./partials/header.ejs %>
    <% include ./partials/nav.ejs %>
    <link rel="stylesheet"
      href="//cdnjs.cloudflare.com/ajax/libs/highlight.js/9.13.1/styles/default.min.css">
<script src="//cdnjs.cloudflare.com/ajax/libs/highlight.js/9.13.1/highlight.min.js"></script>
      <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/github-markdown-css/2.10.0/github-markdown.css">
            <link rel="icon" type="image/jpg" href="https://media.discordapp.net/attachments/506425178822279168/507496898823847936/dbr.png">
            <title>Moderate user - Discord Bot Reviews</title>
    </head>
    <body>
        <center><h2>Moderate <%=username%></h2>
        <br><br>
        <center>
            <%if(bot.userinfo.get(cli_id, "info")) {if(bot.userinfo.get(cli_id, "info").length >0) {%>
          <h3>Biography</h3>
          <p style="padding:8px;background-color: black; width:50%;"><code><%=bot.userinfo.get(cli_id, "info")%></code></p>
<form action="/modportal/biodelete" method="POST">
  <input name="cli_id" type="hidden" value="<%=cli_id%>">
<button class="btn btn-danger">Delete Biography</button>
</form>
<% }}%>
<%if(bot.userinfo.has(cli_id, "website")) { if(bot.userinfo.get(cli_id, "website").length >0){%>
<br><br>
<h3>Personal Website</h3>
<br>
<p style="padding:8px;background-color: black; width:50%;"><code><%=bot.userinfo.get(cli_id, "website")%></code></p>
<form action="/modportal/websitedelete" method="POST">
  <input name="cli_id" type="hidden" value="<%=cli_id%>">
<button class="btn btn-danger">Delete Website</button>
</form>
<% }}%>
<%if(bot.userinfo.has(cli_id, "custompulse")) { if(bot.userinfo.get(cli_id, "custompulse").length >0){%>
  <br><br>
  <h3>Background header image</h3>
  <br>
  <p style="padding:8px;background-color: black; width:50%;"><code><%=bot.userinfo.get(cli_id, "custompulse")%></code></p>
  <img src="<%=bot.userinfo.get(cli_id, "custompulse")%>" width="700" height="250">
  <br><br>
  <form action="/modportal/custompulsedelete" method="POST">
    <input name="cli_id" type="hidden" value="<%=cli_id%>">    
  <button class="btn btn-danger">Delete background</button>
  </form>
  <% }}%>
  <%if(bot.userinfo.has(cli_id, "pulsecolor")) { if(bot.userinfo.get(cli_id, "pulsecolor").length >1){%>
    <br><br>
    <h3>Custom Pulse Color</h3>
    <br>
    <p style="padding:8px;background-color: black; width:50%;"><code><%=bot.userinfo.get(cli_id, "pulsecolor")%></code></p>
    <div style="border-radius:50%; background-color: rgb(<%=bot.userinfo.get(cli_id, "pulsecolor")%>);width:50px;height:50px; "></div>
    <br>
    <form action="/modportal/pulsecolordelete" method="POST">
      <input name="cli_id" type="hidden" value="<%=cli_id%>">
    <button class="btn btn-danger">Delete Custom pulse color</button>
    </form>
    <% }}%>
    <%if(bot.userinfo.has(cli_id, "nametag")) { if(bot.userinfo.get(cli_id, "nametag").length >1){%>
      <br><br>
      <h3>Custom Nametag color</h3>
      <br>
      <p style="padding:8px;background-color: black; width:50%;"><code><%=bot.userinfo.get(cli_id, "nametag")%></code></p>
      <div style="border-radius:50%; background-color: rgb(<%=bot.userinfo.get(cli_id, "nametag")%>);width:50px;height:50px; "></div>
      <br>
      <form action="/modportal/nametagdelete" method="POST">
        <input name="cli_id" type="hidden" value="<%=cli_id%>">
      <button class="btn btn-danger">Delete Custom Nametag color</button>
      </form>
      <% }}%>
      <%if(bot.userinfo.has(cli_id, "commentbubble")) { if(bot.userinfo.get(cli_id, "commentbubble").length >1){%>
        <br><br>
        <h3>Custom Comment color/image</h3>
        <br>
        <p style="padding:8px;background-color: black; width:50%;"><code><%=bot.userinfo.get(cli_id, "commentbubble")%></code></p>
        <%if(bot.userinfo.get(cli_id, "commentbubble").startsWith("http")) {%><img src="<%=bot.userinfo.get(cli_id, "commentbubble")%>"width="700" height="200"><%} else {%><div style="border-radius:50%; background-color: rgb(<%=bot.userinfo.get(cli_id, "commentbubble")%>);width:50px;height:50px; "></div><%}%>
        <br><br>
        <form action="/modportal/commentbubbledelete" method="POST">
          <input name="cli_id" type="hidden" value="<%=cli_id%>">
        <button class="btn btn-danger">Delete Custom Comment Image/color</button>
        </form>
        <% }}%>
        </center>
        <br><br>
        <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js" integrity="sha384-ZMP7rVo3mIykV+2+9J3UJ46jBk0WLaUAdn689aCwoqbBJiSnjAK/l8WvCWPIPm49" crossorigin="anonymous"></script>
        <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js" integrity="sha384-ChfqqxuZUCnJSK3+MXmPNIyE6ZbWh2IMqE241rYiqJxyMiZ6OW/JmZQ5stwEULTy" crossorigin="anonymous"></script>
        </body>
</html>
